{
  "name": "postcss-load-options",
  "version": "0.1.0",
  "description": "Load Options for PostCSS",
  "main": "index.js",
  "scripts": {
    "pretest": "echo '=> Linting' && standard",
    "test": "echo '=> Testing' && nyc ava '*.test.js'",
    "posttest": "echo '=> Cleaning' && rm -rf .nyc_output coverage",
    "start": "echo '=> Starting' && node exec.js"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "nyc": "^6.4.4",
    "standard": "^7.1.1"
  },
  "ava": {
    "babel": "inherit"
  },
  "nyc": {
    "all": true,
    "require": "babel-register",
    "include": [
      "lib",
      "test"
    ],
    "extension": [
      ".js"
    ]
  },
  "standard": {
    "ignore": []
  },
  "babel": {
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    },
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michael-ciniawsky/postcss-load-options.git"
  },
  "keywords": [
    "PostCSS",
    "Options",
    "Autoload"
  ],
  "author": "Michael Ciniawsky <michael.ciniawkygmail.com>",
  "bugs": {
    "url": "https://github.com/michael-ciniawsky/postcss-load-options/issues"
  },
  "homepage": "https://github.com/michael-ciniawsky/postcss-load-options#readme",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "nyc": "^6.4.4",
    "standard": "^7.1.1"
  }
}
